window.addEventListener ("keydown", moveSoemthing, false);

function movePlayer(e) {
  switch(e.keyCode) {
    case 37: //Left arrow key
    break;
    case 38: //Up arrow key
    break;
    case 39: //Right arrow key
    break;
    case 40: //Down arrow key
    break;
  }
}



